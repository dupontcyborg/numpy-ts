{
  "name": "numpy-ts",
  "version": "0.12.0",
  "description": "Complete NumPy implementation for TypeScript and JavaScript (94% API coverage)",
  "main": "dist/numpy-ts.node.cjs",
  "browser": "dist/numpy-ts.browser.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist/**/*.js",
    "dist/**/*.cjs",
    "dist/**/*.mjs",
    "dist/**/*.cjs.map",
    "dist/**/*.mjs.map",
    "dist/esm/**/*.js",
    "dist/types/**/*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": "./dist/numpy-ts.node.cjs",
      "browser": "./dist/numpy-ts.browser.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./node": {
      "types": "./dist/types/node.d.ts",
      "import": "./dist/numpy-ts.node-io.mjs",
      "require": "./dist/numpy-ts.node-io.cjs",
      "default": "./dist/numpy-ts.node-io.cjs"
    },
    "./core": {
      "types": "./dist/types/core.d.ts",
      "import": "./dist/esm/core.js",
      "default": "./dist/esm/core.js"
    }
  },
  "scripts": {
    "generate": "tsx scripts/generate-full.ts && prettier --write src/full/index.ts",
    "build": "npm run build:clean && npm run generate && tsx build.ts && npm run build:types",
    "build:clean": "rm -rf dist",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "test": "npm run build && vitest run",
    "test:unit": "vitest run --project=unit",
    "test:validation": "vitest run --project=validation",
    "test:bundles": "npm run build && vitest run --project=bundle-node --project=bundle-esm --project=bundle-browser",
    "test:tree-shaking": "vitest run --project=tree-shaking",
    "test:ci": "npm run lint && npm run typecheck && npm test",
    "test:coverage": "vitest run --project=unit --project=validation --coverage",
    "test:coverage:view": "open coverage/index.html || xdg-open coverage/index.html",
    "lint": "eslint --cache src tests",
    "lint:fix": "eslint --cache src tests --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "bench:build": "esbuild benchmarks/src/index.ts --bundle --platform=node --format=cjs --outfile=benchmarks/dist/bench.cjs",
    "bench": "npm run bench:build && node benchmarks/dist/bench.cjs",
    "bench:quick": "npm run bench:build && node benchmarks/dist/bench.cjs --quick",
    "bench:large": "npm run bench:build && node benchmarks/dist/bench.cjs --large",
    "bench:category": "npm run bench:build && node benchmarks/dist/bench.cjs --category",
    "bench:view": "open benchmarks/results/plots/latest.html || xdg-open benchmarks/results/plots/latest.html",
    "bench:large:view": "open benchmarks/results/plots/latest-large.html || xdg-open benchmarks/results/plots/latest-large.html",
    "compare-api": "python3 scripts/compare-api-coverage.py"
  },
  "keywords": [
    "numpy",
    "array",
    "ndarray",
    "matrix",
    "linear-algebra",
    "scientific-computing",
    "math",
    "statistics",
    "fft",
    "typescript",
    "javascript"
  ],
  "author": "Nicolas Dupont",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dupontcyborg/numpy-ts.git"
  },
  "bugs": {
    "url": "https://github.com/dupontcyborg/numpy-ts/issues"
  },
  "homepage": "https://github.com/dupontcyborg/numpy-ts",
  "devDependencies": {
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/browser": "^4.0.3",
    "@vitest/browser-playwright": "^4.0.3",
    "@vitest/coverage-v8": "^4.0.3",
    "chart.js": "^4.5.1",
    "chartjs-node-canvas": "^5.0.0",
    "esbuild": "^0.27.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "playwright": "^1.56.1",
    "prettier": "^3.1.1",
    "rollup": "^4.40.0",
    "ts-loader": "^9.5.2",
    "ts-morph": "^27.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3",
    "vitest": "^4.0.3",
    "webpack": "^5.99.9"
  },
  "engines": {
    "node": ">=20.1.0"
  },
  "overrides": {
    "js-yaml": ">=4.1.1"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
